super + Return
	urxvt

super + t
  gnome-terminal

super + b
  brave

super + c
  chromium

super + d
  nautilus >/dev/null

super + f
  firefox

super + n
  rofi -show ssh

super + r
  ~/bin/scripts/rofi-beats

super + s
  spotify


#super + q
#  pkill -USR1 -x sxhkd

#
# Lock the screen
#ctrl + alt + l
#    xscreensaver-command -lock
super + l
    xautolock -locknow

# launch lynx web browser

#Modify to launch rwx lynx
super + alt + l
    st -e lynx -cfg=~/.lynx/lynx.cfg -lss=~/.lynx/lynx.lss -vikeys gopher://distro.tube

super + @space
  rofi -show run

#super + @space
#  dmenu_run


## Requires wmctrl

super + q
  wmctrl -c :ACTIVE:

#switch to workspace
super + 1
  wmctrl -s 1

super + 2
  wmctrl -s 2

super + 3
  wmctrl -s 3

super + 4
  wmctrl -s 4

super + 5
  wmctrl -s 5

super + 6
  wmctrl -s 6

super + 7
  wmctrl -s 7

super + 8
  wmctrl -s 8

super + 9
  wmctrl -s 9

super + 0
  wmctrl -s 0

## Send to a different workspace

super + shift + 0
  wmctrl -r :ACTIVE: -t 0; wmctrl -s 0
super + shift + 1
  wmctrl -r :ACTIVE: -t 1; wmctrl -s 1
super + shift + 2
  wmctrl -r :ACTIVE: -t 2; wmctrl -s 2
super + shift + 3
  wmctrl -r :ACTIVE: -t 3; wmctrl -s 3
super + shift + 4
  wmctrl -r :ACTIVE: -t 4; wmctrl -s 4
super + shift + 5
  wmctrl -r :ACTIVE: -t 5; wmctrl -s 5
super + shift + 6
  wmctrl -r :ACTIVE: -t 6; wmctrl -s 6
super + shift + 7
  wmctrl -r :ACTIVE: -t 7; wmctrl -s 7
super + shift + 8
  wmctrl -r :ACTIVE: -t 8; wmctrl -s 8
super + shift + 9
  wmctrl -r :ACTIVE: -t 9; wmctrl -s 9



