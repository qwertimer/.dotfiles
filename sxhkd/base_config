# Open Help menu
super + question
  ~/.local/bin/scripts/helpsxhkd

# Open terminal
super + Return
	tilix

# Open gnome-terminal
super + t
  gnome-terminal

# Open Brave
super + b
  brave

# Open Chrome
super + c
  chromium

# Open file explorer
super + d
  nautilus >/dev/null

# Open firefox
super + f
  firefox

# Lock the screen
#ctrl + alt + l
#    xscreensaver-command -lock
super + l
    xautolock -locknow

# launch lynx web browser

#Modify to launch rwx lynx
super + alt + l
    urxvt -e lynx -cfg=~/.lynx/lynx.cfg -lss=~/.lynx/lynx.lss -vikeys www.google.com

# Open ssh rofi display
super + n
  rofi -show ssh

# Run rofi beats
super + r
  ~/.local/bin/scripts/rofi-beats

# Open spotify
super + s
  spotify


#super + q
#  pkill -USR1 -x sxhkd

# Opens rofi window search
super + w
   rofi -show window

# Quick task note
#super + period
#  ~/.local/bin/scripts/qnote

# Rofi run menu
super + @space
  rofi -show run

#super + @space
#  dmenu_run


## Requires wmctrl

# Close window
super + q
  wmctrl -c :ACTIVE:

# switch to workspace 1
super + 1
  wmctrl -s 1


# switch to workspace 2
super + 2
  wmctrl -s 2

# switch to workspace 3
super + 3
  wmctrl -s 3

# switch to workspace 4
super + 4
  wmctrl -s 4

# switch to workspace 5
super + 5
  wmctrl -s 5

# switch to workspace 6
super + 6
  wmctrl -s 6

# switch to workspace 7
super + 7
  wmctrl -s 7

# switch to workspace 8
super + 8
  wmctrl -s 8

# switch to workspace 9
super + 9
  wmctrl -s 9

# switch to workspace 0
super + 0
  wmctrl -s 0

## Send to a different workspace

super + shift + 0
  wmctrl -r :ACTIVE: -t 0; wmctrl -s 0
super + shift + 1
  wmctrl -r :ACTIVE: -t 1; wmctrl -s 1
super + shift + 2
  wmctrl -r :ACTIVE: -t 2; wmctrl -s 2
super + shift + 3
  wmctrl -r :ACTIVE: -t 3; wmctrl -s 3
super + shift + 4
  wmctrl -r :ACTIVE: -t 4; wmctrl -s 4
super + shift + 5
  wmctrl -r :ACTIVE: -t 5; wmctrl -s 5
super + shift + 6
  wmctrl -r :ACTIVE: -t 6; wmctrl -s 6
super + shift + 7
  wmctrl -r :ACTIVE: -t 7; wmctrl -s 7
super + shift + 8
  wmctrl -r :ACTIVE: -t 8; wmctrl -s 8
super + shift + 9
  wmctrl -r :ACTIVE: -t 9; wmctrl -s 9



